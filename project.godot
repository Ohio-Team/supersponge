; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Supersponge"
run/main_scene="res://scenes/Main Menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ohio"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
boot_splash/image="res://splash.png"
config/icon="res://icon.png"
boot_splash/minimum_display_time=500

[audio]

bmod/default_sfx_bus=&"SFX"

[autoload]

MusicPlayer="*res://scenes/MusicPlayer.tscn"
Ui="*res://scenes/2d/ui.tscn"
Singleton="*res://scripts/singleton.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"
BMOD="*res://addons/bmod/autoloads/bmod/bmod.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/stretch/aspect="ignore"
window/per_pixel_transparency/allowed=true

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/bmod/plugin.cfg", "res://addons/third-person-camera/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/2d/": "yellow",
"res://assets/3d/": "green",
"res://assets/fonts/": "red",
"res://assets/music/": "pink"
}

[global_group]

Player=""
Bird=""
Enemy=""
rebind_button=""

