resource_name = "Tutorial"
script/source = "extends Node2D


# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	Ui.time = 0
	Singleton.inmenu = false
	Singleton.tokens.clear()
	Singleton.hasgun = false
	Singleton.hashammer = false
	Singleton.save_game()
	Ui._clear_dialog()
	MusicPlayer.play_song(\"res://assets/music/tutorial.ogg\")
	Ui.create_dialog(\"Time to walk boy\",\"sandy\")
	DiscordRPC.details = \"In the Tutorial\"
	DiscordRPC.refresh()
"

